<div class="container">
  <h1>Atlas</h1>
</div>

<div class="banner">
  <div class="container">
    <h2>Continents</h2>
  </div>
</div>

<div class="container">
  <ul>
    <% @continents.each do |continent|%>
      <li>
        <%= link_to continent.name, continent_path(continent.id)%>
      ||  <%= link_to "Edit", edit_continent_path(continent.id)%>
      </li>
    <% end %>
  </ul>
<br>
  <%= link_to "[New continent]", new_continent_path %>
</div>
